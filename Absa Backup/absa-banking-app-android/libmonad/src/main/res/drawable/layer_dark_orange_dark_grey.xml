<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:width="@dimen/small_space">
            <solid android:color="@color/greyish_brown" />
            <corners android:radius="@dimen/tiny_space" />

        </shape>
