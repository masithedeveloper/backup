<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="@dimen/dimen_8dp" android:bottomRightRadius="@dimen/dimen_8dp" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/color_FF374154" />
            <solid android:color="@color/color_FF262F3C" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="@dimen/dimen_8dp" android:bottomRightRadius="@dimen/dimen_8dp" />
            <solid android:color="@color/color_FF999999" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="@dimen/dimen_8dp" android:bottomRightRadius="@dimen/dimen_8dp" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/color_FF262F3C" />
            <solid android:color="@color/color_FF374154" />
        </shape>
    </item>
</selector>