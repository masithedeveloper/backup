<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_8dp" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/color_FF59595B" />
            <solid android:color="@color/color_00000000" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dimen_8dp" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/color_FFFFFFFF" />
            <solid android:color="@color/color_00000000" />
        </shape>
    </item>
</selector>