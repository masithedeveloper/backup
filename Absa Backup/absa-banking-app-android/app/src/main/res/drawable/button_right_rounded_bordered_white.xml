<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:bottomRightRadius="@dimen/dimen_8dp" android:topRightRadius="@dimen/dimen_8dp" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/color_FF999999" />
            <solid android:color="@color/color_33FFFFFF" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:bottomRightRadius="@dimen/dimen_8dp" android:topRightRadius="@dimen/dimen_8dp" />
            <stroke android:width="@dimen/dimen_1dp" android:color="#ECFFFFFF" />
            <solid android:color="#CCFFFFFF" />
        </shape>
    </item>
</selector>