<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="@dimen/dimen_8dp" android:topLeftRadius="@dimen/dimen_8dp" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/color_FF999999" />
            <solid android:color="@color/color_33FFFFFF" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="@dimen/dimen_8dp" android:topLeftRadius="@dimen/dimen_8dp" />
            <stroke android:width="@dimen/dimen_1dp" android:color="@color/light_grey" />
            <solid android:color="@color/light_grey" />
        </shape>
    </item>
</selector>