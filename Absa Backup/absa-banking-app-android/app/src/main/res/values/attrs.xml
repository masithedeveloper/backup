<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="progress" format="integer" />
    <declare-styleable name="TimeOutButton">
        <attr name="duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="CustomBarAttributes">
        <attr name="title_text" format="string" />
    </declare-styleable>
    <declare-styleable name="CompoundButton">
        <attr name="android:button" />
    </declare-styleable>

    <attr name="centered" format="boolean" />
    <attr name="selectedColor" format="color" />
    <attr name="strokeWidth" format="dimension" />
    <attr name="unselectedColor" format="color" />
    <attr name="editable" format="boolean" />

    <declare-styleable name="ViewPagerIndicator">

        <!-- Style of the circle indicator. -->
        <attr name="vpiCirclePageIndicatorStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="CirclePageIndicator">

        <!-- Whether or not the indicators should be centered. -->
        <attr name="centered" />
        <!-- Color of the filled circle that represents the current page. -->
        <attr name="fillColor" format="color" />
        <!-- Color of the filled circles that represents pages. -->
        <attr name="pageColor" format="color" />
        <!-- Orientation of the indicator. -->
        <attr name="android:orientation" />
        <!-- Radius of the circles. This is also the spacing between circles. -->
        <attr name="radius" format="dimension" />
        <!-- Whether or not the selected indicator snaps to the circles. -->
        <attr name="snap" format="boolean" />
        <!-- Color of the open circles. -->
        <attr name="strokeColor" format="color" />
        <!-- Width of the stroke used to draw the circles. -->
        <attr name="strokeWidth" />
    </declare-styleable>

    <declare-styleable name="SegmentButtons">
        <attr name="permission_type" format="string" />
        <attr name="permission_visibility" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PleaseSelectStyle">
        <attr name="text_appearance" format="string" />
    </declare-styleable>

    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="umanoPanelHeight" format="dimension" />
        <attr name="umanoShadowHeight" format="dimension" />
        <attr name="umanoParalaxOffset" format="dimension" />
        <attr name="umanoFadeColor" format="color" />
        <attr name="umanoFlingVelocity" format="integer" />
        <attr name="umanoDragView" format="reference" />
        <attr name="umanoScrollableView" format="reference" />
        <attr name="umanoOverlay" format="boolean" />
        <attr name="umanoClipPanel" format="boolean" />
        <attr name="umanoAnchorPoint" format="float" />
        <attr name="umanoInitialState" format="enum">
            <enum name="expanded" value="0" />
            <enum name="collapsed" value="1" />
            <enum name="anchored" value="2" />
            <enum name="hidden" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BottomSheet">
        <attr name="bottomSheetStyles" format="reference" />
        <attr name="bs_dialogBackground" format="color|reference" />
        <attr name="bs_listStyle" format="reference" />
        <attr name="bs_dividerColor" format="color|reference" />
        <attr name="bs_numColumns" format="integer" />
        <attr name="bs_titleTextAppearance" format="reference" />
        <attr name="bs_listItemTitleTextAppearance" format="reference" />
        <attr name="bs_gridItemTitleTextAppearance" format="reference" />
        <attr name="bs_moreDrawable" format="reference" />
        <attr name="bs_moreText" format="string" />
        <attr name="bs_closeDrawable" format="reference" />
        <attr name="bs_collapseListIcons" format="boolean" />
        <attr name="bs_listItemLayout" format="reference" />
        <attr name="bs_gridItemLayout" format="reference" />
        <attr name="bs_headerLayout" format="reference" />
    </declare-styleable>

    <declare-styleable name="FiveStepProgressBarSettings">
        <attr name="progressOutOfFive" format="integer" />
    </declare-styleable>

    <declare-styleable name="HintSelectTabSettings">
        <attr name="titleField1" format="string" />
        <attr name="hintField" format="string" />
    </declare-styleable>

    <declare-styleable name="TwoColumnRoundedTabSettings">
        <attr name="titleField2" format="string" />
        <attr name="captionLeft" format="string" />
        <attr name="captionRight" format="string" />
        <attr name="subheadings" format="string" />
        <attr name="subheadingLeftValue" format="string" />
        <attr name="subheadingRightValue" format="string" />
    </declare-styleable>

    <declare-styleable name="SeekBarRoundedTabSettings">
        <attr name="title1" format="string" />
        <attr name="leftCaption" format="string" />
        <attr name="leftSubHeadingLeftField" format="string" />
        <attr name="centerCaption" format="string" />
        <attr name="centerSubHeadingLeftField" format="string" />
        <attr name="centerSubHeadingRightField" format="string" />
        <attr name="rightCaption" format="string" />
        <attr name="rightSubHeadingLeftField" format="string" />
    </declare-styleable>

    <declare-styleable name="ReferenceTabSettings">
        <attr name="referenceTitle" format="string" />
        <attr name="referenceHint" format="string" />
    </declare-styleable>

    <declare-styleable name="MaskedEditText">
        <attr name="mask" format="string" />
        <attr name="char_representation" format="string" />
        <attr name="mask_fill" format="string" />
    </declare-styleable>

    <declare-styleable name="CountDownCircularView">
        <attr name="count_down_duration" format="integer" />
        <attr name="android:textSize" />
        <attr name="android:color" />
        <attr name="half_diameter" format="dimension" />
        <attr name="outline_color" format="color" />
        <attr name="stroke_width" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="seconds_text_size" format="dimension" />
        <attr name="seconds_color" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CreditLimitIndicatorView">
        <attr name="animation_duration" format="integer" />
        <attr name="indicator_text_color" format="color" />
        <attr name="track_color" format="color" />
        <attr name="progress_indicator_color" format="color" />
        <attr name="background_arc_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CreditCardGraph">
        <attr name="attribute_animation_duration" format="integer" />
        <attr name="attribute_track_color" format="color" />
        <attr name="attribute_progress_indicator_color" format="color" />
    </declare-styleable>


</resources>
